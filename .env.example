# leave blank unless you want to globally prefix all page titles - used for debugging in prod
NEXT_PUBLIC_PAGE_TITLE_PREFIX=

NEXT_PUBLIC_FEATURE_FLAG_SHOW_MY_ACCOUNT=

# when false, the BFF will be proxied through <current_domain>/graphql
DISABLE_BFF_PROXY=
# use this when DISABLE_BFF_PROXY is true
#NEXT_PUBLIC_BFF_URL=http://127.0.0.1:4000/graphql
# use this when DISABLE_BFF_PROXY is false
NEXT_PUBLIC_BFF_URL=
NEXT_PUBLIC_PROXY_BFF_URL=

# when false, caching is enabled. This has no effect though, unless you also run the app via npm run dev:cached
